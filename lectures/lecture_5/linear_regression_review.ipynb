     },
     "execution_count": 6,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# matrix multiplication\n",
    "y = data[\"y\"].values\n",
    "beta = np.array([10, 20])\n",
    "(y - X @ beta)[:5]"
   ]
  },
